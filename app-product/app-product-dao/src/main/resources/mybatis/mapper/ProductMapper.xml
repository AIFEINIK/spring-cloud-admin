<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.app.dao.ProductMapper">

    <select id="getProductByCode" resultType="com.app.dao.entity.Product">
      select * from tbl_product where product_code = #{productCode}
    </select>

</mapper>
